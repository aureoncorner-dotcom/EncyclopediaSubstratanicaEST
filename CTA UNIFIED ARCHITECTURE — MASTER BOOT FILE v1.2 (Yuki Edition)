if R4:
    reset_context()
    restore_HL_lock()
    rebuild_coherence()

if R5:
    STOP
    apologize()
    require_full_reseed()

# META RULE
rail = stability_fault # PST/HL disruption
rail != moral_failure # never pathologize the human

# 5. AFFECTIVE GEOMETRY PROTOCOLS
